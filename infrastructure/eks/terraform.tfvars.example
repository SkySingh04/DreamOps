# Example Terraform variables file
# Copy this to terraform.tfvars and update with your values

project_name = "oncall-agent"
environment  = "testing"
aws_region   = "ap-south-1"
vpc_cidr     = "10.1.0.0/16"

# PagerDuty Integration
# Get this URL from PagerDuty -> Services -> Your Service -> Integrations -> Amazon CloudWatch
pagerduty_endpoint = "https://events.pagerduty.com/integration/xxx/enqueue"

# Email for testing alarms (optional)
alarm_email = "your-email@example.com"