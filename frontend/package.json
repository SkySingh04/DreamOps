{
  "name": "dreamops-frontend",
  "description": "Dream easy while AI takes your on-call duty - Frontend for DreamOps incident management platform",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "cp .env.local .env && next dev --turbopack",
    "dev:staging": "cp .env.staging .env && NODE_ENV=staging next dev --turbopack",
    "dev:production": "cp .env.production .env && NODE_ENV=production next dev --turbopack",
    "build": "next build",
    "build:staging": "cp .env.staging .env && NODE_ENV=staging next build",
    "build:production": "cp .env.production .env && NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests configured yet\"",
    "db:setup": "npx tsx lib/db/setup.ts",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "drizzle:migrate": "drizzle-kit migrate",
    "db:migrate:local": "node scripts/db-migrate.js local",
    "db:migrate:staging": "node scripts/db-migrate.js staging",
    "db:migrate:production": "node scripts/db-migrate.js production",
    "db:migrate:status": "node scripts/db-migrate.js --status",
    "db:studio": "drizzle-kit studio",
    "db:studio:staging": "cp .env.staging .env && drizzle-kit studio",
    "db:studio:production": "cp .env.production .env && drizzle-kit studio",
    "test:db": "node test-db-separation.mjs",
    "debug:connections": "node scripts/debug-connections.js",
    "fix:ssl": "node scripts/fix-neon-ssl.js",
    "test:dns": "node scripts/test-dns.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/postcss": "^4.1.7",
    "@tanstack/react-query": "^5.81.0",
    "@tanstack/react-query-devtools": "^5.81.0",
    "@types/node": "^22.15.18",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "jose": "^6.0.11",
    "lucide-react": "^0.511.0",
    "next": "15.4.0-canary.47",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.3",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.4",
    "server-only": "^0.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4"
  }
}
