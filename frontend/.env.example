# Environment
NODE_ENV=development
NEXT_PUBLIC_DEV_MODE=true

# Authentication Secret - Change this in production!
AUTH_SECRET=your-super-secret-jwt-key-change-this-in-production-make-it-at-least-32-characters-long

# Database URL
POSTGRES_URL=postgresql://oncall_user:changeme@localhost:5432/oncall_agent_dev

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_WS_URL=http://localhost:8000

# PagerDuty Integration
PAGERDUTY_ENABLED=false
PAGERDUTY_WEBHOOK_SECRET=dev-webhook-secret
PAGERDUTY_API_KEY=dev-api-key
PAGERDUTY_USER_EMAIL=dev@yourcompany.com

# Payment Configuration
# Set to true to use mock payments for testing, false for real PhonePe integration
NEXT_PUBLIC_USE_MOCK_PAYMENTS=true

# Firebase Configuration (Development Project)
NEXT_PUBLIC_FIREBASE_API_KEY=AIzaSyDev-api-key-here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-dev-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-dev-project
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-dev-project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:devappid
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-DEVMEASUREMENT

# Optional: Analytics, monitoring, etc.
# NEXT_PUBLIC_GA_ID=your-google-analytics-id