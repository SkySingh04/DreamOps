{
  "dashboard": {
    "id": null,
    "uid": "test-dashboard-001",
    "title": "OnCall Agent Test Dashboard",
    "tags": ["test", "oncall-agent"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Service Uptime",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "up",
            "interval": "",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "interval": "",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "interval": "",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 4,
        "title": "Test Data Panel",
        "type": "timeseries",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "datasource": {
              "type": "testdata",
              "uid": "testdata-datasource"
            },
            "scenarioId": "random_walk",
            "seriesCount": 1,
            "alias": "Test Metric"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  },
  "folderId": 0,
  "overwrite": false
}