# EKS Kubeconfig Export
# Source this file to use the EKS cluster: source .env.eks

# Path to the EKS kubeconfig file
export KUBECONFIG=/Users/akashsingh/Desktop/oncall-agent/infrastructure/eks/kubeconfig-oncall-agent-eks.yaml

# AWS Profile to use for EKS authentication
export AWS_PROFILE=burner

# AWS Region for the EKS cluster
export AWS_DEFAULT_REGION=ap-south-1

# EKS Cluster Context
export K8S_CONTEXT=arn:aws:eks:ap-south-1:500489831186:cluster/oncall-agent-eks

# Test namespace for sample apps
export K8S_NAMESPACE=oncall-test-apps

# Instructions for teammates:
# 1. Get AWS credentials for the 'burner' profile from your team lead
# 2. Configure AWS profile: aws configure --profile burner
# 3. Source this file: source .env.eks
# 4. Test connection: kubectl get nodes